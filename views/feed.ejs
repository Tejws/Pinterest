<% include ./partials/header.ejs %>
<div class="w-full min-h-screen  bg-zinc-800 px-10 py-8 text-white">

    <div class="cards flex flex-wrap gap-10">
        <% posts.forEach(function(elem){%>
            <div class="w-64 h-64 bg-zinc-800 rounded-lg object-cover shadow-md  mt-5 ">
            <div class="w-64 h-64 bg-zinc-800 rounded-lg overflow-hidden object-cover">
                <img  src="/images/uploads/<%= elem.image %>" alt="">
            </div>
            <h5 class="mt-2 text-sm"><%=elem.title%></h5>
        </div>
       <% })%>
       
    
        
        
    </div>
    </div>
<% include ./partials/footer.ejs %>